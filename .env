# Примеры URL и API настроек
REACT_APP_BASE_URL=https://api.example.com/api/courses/
REACT_APP_BASE_URL_HOST=https://api.example.com
REACT_APP_HOST=api.example.com/api/courses/
REACT_APP_API_URL=https://api.coursehub.example/api
REACT_APP_RUN_MODE=DEV
# REACT_APP_RUN_MODE=PRODUCTION

# Пример данных доступа
RUSSIAN_LOGIN=your_login
RUSSIAN_PASSWORD=your_password

BY_TOKEN=your_by_token
ALFA_SMS=your_sms_code

EMAIL_NAME=noreply@example.com
EMAIL_PASSWORD=your_email_password

DB_URL=postgres://user:password@localhost:5432/db_name
SECRET_KEY=your_secret_key
DEBUG=TRUE

# Redis конфигурация
REDIS_HOST=redis
REDIS_PORT=6379

# URL сайта
SITE_URL=http://sandbox.example.com

# BePaid платёжная система
BEPAID_SHOP_ID=your_shop_id
BEPAID_SECRET_KEY=your_bepaid_secret_key
NOTIFICATION_URL_BEPAID=http://sandbox.example.com/api/payment-notification/

# Подпись токенов
ALGORITHM=HS256
SIGNING_KEY=your_jwt_signing_key

# PostgreSQL БД
POSTGRES_DB_NAME=your_db
POSTGRES_USER=your_user
POSTGRES_USER_PASSWORD=your_db_password
POSTGRES_HOST=your_db_host
POSTGRES_PORT=5432

# S3 хранилище
S3_ACCESS_KEY=your_s3_access_key
S3_SECRET_KEY=your_s3_secret_key
REGION_NAME=ru-1
ENDPOINT_URL=https://s3.example.com
S3_BUCKET=your_bucket_name

# Яндекс OAuth
YANDEX_TOKEN=your_yandex_token
YANDEX_CLIENT_ID=your_yandex_client_id
YANDEX_SECRET_KEY=your_yandex_secret_key

# Общая авторизация
CLIENT_ID=your_client_id
SEL_AUTH_KEY=your_sel_auth_key
ACCOUNT_ID=your_account_id
CONTAINER_KEY=your_container_key

# Telegram Bot
TG_BOT_TOKEN=your_telegram_bot_token

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_SECRET=your_google_client_secret

# YouTube API
YOUTUBE_API_KEY=your_youtube_api_key

